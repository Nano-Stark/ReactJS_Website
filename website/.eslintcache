tton className=\"btn btn-default\" onClick={e => {\r\n                                    this.props.getPosts(this.props.site.posts.length);\r\n                                }}>Load More</button>\r\n                            : null}\r\n                            \r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            </section>\r\n\r\n                \r\n           </div>\r\n            )\r\n    }\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    site: state.site\r\n})\r\n\r\nconst mapDispatchToProps = dispatch => ({\r\n    getPosts: (skip) => {\r\n        dispatch(SiteActions.getPosts(skip));\r\n    },\r\n    getPostCount: () => {\r\n        dispatch(SiteActions.getPostCount());\r\n    }\r\n})\r\n\r\nexport default connect(\r\n    mapStateToProps,\r\n    mapDispatchToProps,\r\n    )(withRouter(Blog));","C:\\Users\\User\\NODE_PROJECT\\demo-3-website\\src\\store\\reducers\\siteReducer.js",[],"C:\\Users\\User\\NODE_PROJECT\\demo-3-website\\src\\store\\actions\\siteActions.js",[],"C:\\Users\\User\\NODE_PROJECT\\demo-3-website\\src\\component\\Common\\BlogItem.js",[],"C:\\Users\\User\\NODE_PROJECT\\demo-3-website\\src\\component\\Pages\\SignUp.js",[],"C:\\Users\\User\\NODE_PROJECT\\demo-3-website\\src\\component\\Common\\CommentBuilder.js",[],{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","replacedBy":"201"},{"ruleId":"198","replacedBy":"202"},{"ruleId":"200","replacedBy":"203"},{"ruleId":"198","replacedBy":"204"},{"ruleId":"200","replacedBy":"205"},{"ruleId":"206","severity":1,"message":"207","line":7,"column":8,"nodeType":"208","messageId":"209","endLine":7,"endColumn":13},{"ruleId":"206","severity":1,"message":"210","line":12,"column":8,"nodeType":"208","messageId":"209","endLine":12,"endColumn":12},{"ruleId":"206","severity":1,"message":"211","line":13,"column":8,"nodeType":"208","messageId":"209","endLine":13,"endColumn":16},{"ruleId":"206","severity":1,"message":"212","line":10,